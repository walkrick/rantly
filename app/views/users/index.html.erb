<%= link_to "Logout", root_path, :id => "reg-btn", :class => "inbox-sf-add-btn tip" %>

<%= notice %>

<div class="wrapper">
  <section class="clearfix">
    <article class="two-third">
      <h1><%= current_user.first_name %> <%= current_user.last_name %></h1>
      <%= form_for [@user, @rant], url: rants_path(current_user.id) do |f| %>

        <div>
          <%= f.text_field :rant_title, autofocus: true, placeholder: "Rant about what?" %>
        </div>
        <div>
          <%= f.text_area :rant, autofocus: true, placeholder: "Rant about something or at someone..." %>
        </div>
        <div>
          <%= f.submit "Rant" %>
        </div>
      <% end %>

      <table class="table">
        <thead>
        <h3>Ranters</h3>
        </thead>
        <tbody>
        <% @users.each do |user| %>
          <tr class="reg_link">
            <td><%= link_to (user).username, user_path(user) %></td>
          </tr>
        <% end %>
        </tbody>
      </table>
      <!--</article>-->

      <!--</section>-->
