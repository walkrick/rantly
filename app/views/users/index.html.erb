      <!--&lt;!&ndash;<article class="one_third">&ndash;&gt;-->

      <!--&lt;!&ndash;<h1>Latest Rants</h1>&ndash;&gt;-->

      <!--&lt;!&ndash;</div>&ndash;&gt;-->

      <!--</article>-->

</div>