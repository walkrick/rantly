<section class="dashboard_section">
  <h2 style="border-bottom: solid 1px">Following</h2>
<div>
  <% @following.each do |following| %>
   <div> <%= following.followed_user.first_name%> <%= following.followed_user.last_name%><br>
    <img src=<%= following.followed_user.image%>width="100" height="100"/></div>
    <%= follow_link (following.followed_user) %>
    </div>

  <% end %>
</section>